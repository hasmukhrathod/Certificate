<p id="notice"><%= notice %></p>

<h1><center>PEC Generations</center></h1>

<table class="TFtable">
  <thead>
    <tr>
      <th>PEC No.</th>
      <th>Surname</th>
      <th>Name</th>
      <th>Last Exam Name</th>
      <th>Last Exam Passed </th>
      <th>Last Exam Board/University</th>
      <th>Current Branch</th>
      <th>Current Institute</th>
      <th>Enteredby</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @provisionals.each do |provisional| %>
      <tr>
	<td>PU/PEC/<%= provisional.id %></td> 
        <td><%= provisional.fname %></td>
        <td><%= provisional.mname %></td>
        <td><%= provisional.lastExamName %></td>
        <td><%= provisional.lastExamPass.strftime("%B %Y") %></td>
	<td><%= provisional.lastExamUniversity %></td>
        <td><%= provisional.constituent_class %></td>
        <td><%= provisional.constituentInstituteName %></td>
        <td><%= provisional.enteredBy %></td>

        <td><%= link_to 'PDF', provisional_path(provisional,format: 'pdf') %></td>
          
      </tr>
    <% end %>
  </tbody>
</table>

